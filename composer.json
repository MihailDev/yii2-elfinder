{
    "name": "fonclub/yii2-elfinder",
    "keywords": ["yii", "elfinder", "filemanager"],
    "description": "Yii2 ElFinder",
    "homepage": "https://github.com/fonclub/yii2-elfinder",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "minimum-stability":"stable",
    "authors": [
        {
            "name": "Mihail",
            "email": "mihail.kucher@gmail.com",
            "homepage": "https://github.com/MihailDev",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {"fonclub\\elfinder\\": ""}
    },
    "require": {
        "ext-phar": "*",
        "yiisoft/yii2-jui": "*",
        "2amigos/yii2-transliterator-helper": "*"
    },
    "extra": {
      "asset-installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    }
}
